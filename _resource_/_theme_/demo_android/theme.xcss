	-nexa-text-align : center;
	-nexa-vertical-align : middle;
}

.TitleBarControl .maxbutton[userstatus=pushed]
{
	background : #2196f3;
	-nexa-border : 1px solid transparent;
	border-radius : 50px;
	-nexa-icon : URL("theme://images/btn_max_P.png");
}

.TitleBarControl .maxbutton[status=mouseover]
{
	background : #64b5f6;
	-nexa-border : 1px solid transparent;
	border-radius : 50px;
	-nexa-icon : URL("theme://images/btn_max_O.png");
}

.TitleBarControl .maxbutton[status=disabled]
{
	background : #64b5f6;
	-nexa-border : 1px solid transparent;
	border-radius : 50px;
	-nexa-icon : URL("theme://images/btn_max_D.png");
}

.TitleBarControl .normalbutton
{
	-nexa-icon : URL("theme://images/btn_normal_N.png");
	-nexa-text-align : center;
	-nexa-vertical-align : middle;
}

.TitleBarControl .normalbutton[userstatus=pushed]
{
	background : #2196f3;
	-nexa-border : 1px solid transparent;
	border-radius : 50px;
	-nexa-icon : URL("theme://images/btn_normal_P.png");
}

.TitleBarControl .normalbutton[status=mouseover]
{
	background : #64b5f6;
	-nexa-border : 1px solid transparent;
	border-radius : 50px;
	-nexa-icon : URL("theme://images/btn_normal_O.png");
}

.TitleBarControl .normalbutton[status=disabled]
{
	background : #64b5f6;
	-nexa-border : 1px solid transparent;
	border-radius : 50px;
	-nexa-icon : URL("theme://images/btn_normal_D.png");
}

.TitleBarControl .closebutton
{
	-nexa-border : 1px solid transparent;
	border-radius : 50px;
	-nexa-icon : URL("theme://images/btn_close_N.png");
	-nexa-text-align : center;
	-nexa-vertical-align : middle;
}

.TitleBarControl .closebutton[userstatus=pushed]
{
	background : #b13138;
	-nexa-border : 1px solid transparent;
	border-radius : 50px;
	-nexa-icon : URL("theme://images/btn_close_P.png");
}

.TitleBarControl .closebutton[status=mouseover]
{
	background : #de3641;
	-nexa-border : 1px solid transparent;
	border-radius : 50px;
	-nexa-icon : URL("theme://images/btn_close_O.png");
}

.TitleBarControl .closebutton[status=disabled]
{
	background : #64b5f6;
	-nexa-border : 1px solid transparent;
	border-radius : 50px;
	-nexa-icon : URL("theme://images/btn_close_D.png");
}


/**********************StatusBar***********************************************/
.StatusBarControl
{
	background : #f6f6f6;
	-nexa-border : 1px solid #e0e0e0;
	border-radius : 0px 0px 3px 3px / 0px 0px 3px 3px;
	-nexa-padding : 0px 0px 0px 10px;
}

.StatusBarControl .statustext
{
	-nexa-text-align : left;
	-nexa-vertical-align : middle;
}

.StatusBarControl .resizegrip
{
	-nexa-icon : url("theme://images/img_grip.png");
	-nexa-vertical-align : bottom;
	-nexa-text-align : center;
}

.StatusBarControl .progressbar
{
	background : #90caf9;
	-nexa-border : 5px solid transparent;
	color : #000000;
}

.StatusBarControl .progressbar .progressbaritem
{
	background : #1565c0;
	-nexa-text-align : center;
	-nexa-vertical-align : middle;
}

.StatusBarControl .progressbar .progressbartext
{
	font : bold 8pt "NanumGothic";
	color : #ffffff;
	-nexa-text-align : left;
	-nexa-vertical-align : middle;
}


/**********************Static***********************************************/
.Static,.StaticControl
{
	font : normal 18pt/normal "NanumGothic";
	color : #333333;
	-nexa-text-align : left;
	-nexa-vertical-align : middle;
}

.Static[status=disabled],.StaticControl[status=disabled]
{
	color : #9a9a9a;
}


/**********************Button***********************************************/
.Button
{
	background : #ffffff;
	-nexa-border : 1px solid #c0c0c0;
	border-radius : 2px;
	-nexa-text-align : center;
	-nexa-vertical-align : middle;
	font : normal 16pt/normal "Arial";
}

.Button[status=mouseover]
{
	-nexa-border : 1px solid #9f9f9f;
}

.Button[userstatus=pushed]
{
	background : #bdbdbd;
	-nexa-border : 1px solid #666666;
}

.Button[status=disabled]
{
	background : #eeeeee;
	color : #939393;
}


/**********************Edit/MaskEdit***********************************************/
.Edit
{
	background : #ffffff;
	-nexa-border : 0px none, 0px none, 1px solid #c0c0c0, 0px none;
	-nexa-padding : 0px 3px 0px 3px;
	-nexa-text-align : left;
	font : normal 18pt/normal "Arial";
}

.MaskEdit
{
	background : #ffffff;
	-nexa-border : 0px none, 0px none, 1px solid #c0c0c0, 0px none;
	-nexa-padding : 0px 3px 0px 3px;
	-nexa-text-align : right;
}

.Edit[status=focused],.Edit[status=mouseover],.MaskEdit[status=focused],.MaskEdit[status=mouseover]
{
	-nexa-border : 0px none, 0px none, 2px solid #1565c0, 0px none;
}

.Edit[status=readonly]
{
	-nexa-border : 0px none, 0px none, 1px dashed #c0c0c0, 0px none;
}

.Edit[status=disabled],.MaskEdit[status=disabled]
{
	color : #9a9a9a;
	-nexa-border : 0px none, 0px none, 1px dashed #c0c0c0, 0px none;
}
/**********************TextArea***********************************************/
.TextArea,.TextAreaControl
{
	background : #ffffff;
	-nexa-border : 1px solid #bdbdbd;
	border-radius : 2px;
	-nexa-padding : 3px 3px 3px 3px;
	-nexa-text-align : left;
}

.TextArea[status=mouseover],.TextAreaControl[status=mouseover]
{
	-nexa-border : 1px solid #1565c0;
}

.TextArea[status=disabled],.TextAreaControl[status=disabled]
{
	background : #ebebeb;
	-nexa-border : 1px solid #cccccc;
	color : #999999;
}

.TextArea[status=readonly],.TextAreaControl[status=readonly]
{
	background : #fafafa;
	-nexa-border : 1px solid #c0c0c0;
	color : #333333;
}


/**********************Spin***********************************************/
.Spin
{
	background : #ffffff;
	-nexa-border : 1px solid #c0c0c0;
	border-radius : 2px;
	font : normal 16pt/normal "Arial";
}

.Spin[status=readonly]
{
	background : #fafafa;
}

.Spin[status=disabled]
{
	background : #ebebeb;
	-nexa-border : 1px solid #cccccc;
}

.Spin[status=mouseover]
{
	-nexa-border : 1px solid #136cc8;
}

.Spin .spinedit
{
	-nexa-padding : 0px 3px 0px 3px;
	-nexa-text-align : right;
}

