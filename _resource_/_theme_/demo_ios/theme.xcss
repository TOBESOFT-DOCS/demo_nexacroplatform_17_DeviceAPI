	-nexa-border : 1px solid #dcdcdc;
}


/**********************Sketch***********************************************/
.Sketch,.SketchControl
{
	background : #ffffff;
	-nexa-border : 1px solid #dcdcdc;
	border-radius : 5px;
	color : #6e6e70;
}

.Sketch[status=disabled],.SketchControl[status=disabled]
{
	background : #efeff4;
	color : #aeaeae;
}
