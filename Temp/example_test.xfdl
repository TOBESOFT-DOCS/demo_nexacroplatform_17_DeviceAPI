this.test_onload = function(obj:nexacro.Form,e:nexacro.LoadEventInfo)
{
	var curDate = new Date();
	
	var year = curDate.getFullYear().toString().padLeft(4, "0");
	var month = (curDate.getMonth()+1).toString().padLeft(2, "0");
	var day = curDate.getDate().toString().padLeft(2, "0");
	
	this.Tab00.Tabpage2.form.Calendar00.set_value(year+month+day);
	this.Tab00.Tabpage2.form.Calendar01.set_value(year+month+day);
	this.Tab00.Tabpage2.form.Calendar02.set_value(year+month+day);	
	
};
